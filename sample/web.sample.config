<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <system.webServer>
    <rewrite>
     <rules>
      <rule name="Default" stopProcessing="true">
       <match url="^www/.*$" negate="true" />
       <action type="Rewrite" url="www/index.php" />
      </rule>
     </rules>
    </rewrite>
  </system.webServer>
</configuration>
