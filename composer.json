{
    "name": "snje1987/minifw2",
    "type": "library",
    "description": "A simple php framework",
    "keywords": ["php", "framework"],
    "homepage": "https://github.com/snje1987/minifw2",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Yang Ming",
            "email": "root@snje.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "matthiasmullie/minify": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "squizlabs/php_codesniffer": "^3.0.2",
        "wimg/php-compatibility": "*"
    },
    "autoload": {
        "psr-4": {
            "Org\\Snje\\Minifw\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Org\\Snje\\MinifwTest\\": "tests/"
        }
    }
}